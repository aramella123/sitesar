{
	"configVersion": "1",
	"https": true,
	"templateVersion": 8,
	"locales": [
		"en"
	],
	"features": [
		{
			"name": "KG HBS EPS Home Page",
			"streamLabel": "pages-index",
			"handlebarsTemplateEntityPageSet": {
				"template": "index",
				"urlTemplate": "index-url"
			}
		},
		{
			"name": "KG HBS EPS Locations",
			"streamLabel": "pages-locations",
			"handlebarsTemplateEntityPageSet": {
				"template": "location",
				"urlTemplate": "url"
			}
		},
		{
			"name": "KG HBS EPS Cities",
			"streamLabel": "pages-cities",
			"handlebarsTemplateEntityPageSet": {
				"template": "city",
				"urlTemplate": "url"
			}
		},
		{
			"name": "KG HBS EPS States",
			"streamLabel": "pages-states",
			"handlebarsTemplateEntityPageSet": {
				"template": "state",
				"urlTemplate": "url"
			}
		}
	],
	"ci_config": {
		"base_image_tag": "node-12.18",
		"output_directories": [ "desktop", "templates" ],
		"build_setup_cmd": "npm install",
		"build_cmd": "npm run build",
		"serve_setup_cmd": "echo setup",
		"serve_cmd": "echo serve"
	}
}
